{
  "name": "@glitterprotocol/glitter.proto",
  "version": "1.0.8-beta.3",
  "description": "JS and TS types relating to Protocol Buffers used by Glitter project",
  "contributors": [
    "Rrbin"
  ],
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "alliance/",
    "confio/",
    "cosmos/",
    "cosmos_proto/",
    "cosmwasm/",
    "gogoproto/",
    "google/",
    "ibc/",
    "index/",
    "tendermint/",
    "ethermint/",
    "*.md"
  ],
  "scripts": {
    "format": "prettier --write --loglevel warn \"./src/**/*.ts\"",
    "build": "rm -rf ./build && tsc --module commonjs && ./scripts/prepare.sh",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@improbable-eng/grpc-web": "^0.14.1",
    "google-protobuf": "^3.17.3",
    "long": "^4.0.0",
    "protobufjs": "~6.11.2"
  },
  "devDependencies": {
    "@types/google-protobuf": "^3.15.5",
    "@types/long": "^4.0.1",
    "@types/node": "^15.6.2",
    "prettier": "^2.3.0",
    "ts-proto": "^1.82.5",
    "typescript": "~4.0"
  },
  "main": "index.js",
  "author": ""
}
